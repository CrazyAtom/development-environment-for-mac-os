{
  "terminal.integrated.fontFamily": "Meslo LG M for Powerline",
  "terminal.integrated.fontSize": 14,
  "editor.fontFamily": "'D2Coding ligature'",
  "editor.inlineHints.fontFamily": "'D2Coding ligature'",
  "editor.fontSize": 12,
  "editor.fontLigatures": true,
  "editor.rulers": [
    80,
    120
  ],
  "editor.minimap.enabled": false,
  "editor.accessibilitySupport": "off",
  "window.restoreWindows": "all",
  "javascript.format.enable": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.DS_Store": true,
    "*/node_modules": true,
    "**/.idea": true,
    "**/.vscode": false,
    "**/yarn.lock": true,
    "**/tmp": true,
    "node_modules": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/tmp": true,
    "**/build": true,
    "**/.fvm": true
  },
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/tmp": true,
    "**/coverage": true,
    "**/build": true,
    "**/Pods": true,
    "**/*.xcodeproj": true,
    "**/*.xcworkspace": true,
    "**/.meteor": true,
    "**/.fvm": true
  },
  "editor.formatOnSave": true,
  "extensions.autoUpdate": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.formatOnType": true
  },
  "cSpell.enableFiletypes": ["!markdown"],
  "python.languageServer": "Pylance",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.bracketPairColorization.enabled": true
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[cpp]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4,
    "editor.wordBasedSuggestions": "off",
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "cSpell.userWords": ["asdfasdfasdfasdf"],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "redhat.telemetry.enabled": false,
  "swaggerViewer.previewInBrowser": true,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.showTabs": "multiple",
  "workbench.iconTheme": "vscode-icons",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "git.openRepositoryInParentFolders": "always",
  "dart.debugSdkLibraries": false,
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.flutterSdkPaths": [
    "/Users/crazyatom/fvm/versions"
  ],
  "dart.previewHotReloadOnSaveWatcher": true,
  "git-blame.config.logClickUrls": {},
  "git-blame.config.showBlameInline": false,
  "git-blame.gitWebUrl": "\"\"",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
      "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
      "*.dart": "${capture}.g.dart,${capture}.freezed.dart"
  },
  "editor.stickyScroll.enabled": true,
  "cmake.configureOnOpen": true,
  "files.autoSave": "afterDelay",
  // For Monorepo ESLint
  "eslint.workingDirectories": [
    { "mode": "auto" }
  ],
}
